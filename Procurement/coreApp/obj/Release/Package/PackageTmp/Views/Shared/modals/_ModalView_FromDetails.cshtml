@model string

<div class="modal modal-view-details" id="@Model" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h6 class="modal-title"></h6>
            </div>
            <div class="modal-body clearfix"></div>
            <div class="progress-ui"></div>
             <div class="modal-footer btn-group-sm">
                <button type="button" data-dismiss="modal" class="btn btn-default btn-cancel">Cancel</button>
                <button type="button" class="btn btn-primary btn-submit">Save</button>
            </div>
        </div>
    </div>
</div>  