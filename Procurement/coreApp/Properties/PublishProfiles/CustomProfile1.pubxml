<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://localhost/skyhr</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>localhost</MSDeployServiceURL>
    <DeployIisAppPath>skyhr</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>InProc</MSDeployPublishMethod>
    <EnableMSDeployBackup>False</EnableMSDeployBackup>
    <UserName />
    <_SavePWD>False</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="DefaultConnection" Order="1" Enabled="False">
          <Destination Path="Data Source=tcp:skyhr.database.windows.net;Initial Catalog=skyhr;User ID=skyhrAdmin;Password=IAlone@00" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="coreApp.ApplicationDbContext, coreApp" Origin="Configuration" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="Procurement" Order="2" Enabled="False">
          <Destination Path="Data Source=.\MSSQLSERVER2016;Initial Catalog=procurement_lnu;Integrated Security=True" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source=.\mssqlserver2016;Initial Catalog=procurement_lnu;Integrated Security=True" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\Procurement_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="Data Source=.\mssqlserver2016;Initial Catalog=procurement_lnu;Integrated Security=True" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="Procurement-Web.config Connection String">
      <ParameterValue>Data Source=.\MSSQLSERVER2016;Initial Catalog=procurement_lnu;Integrated Security=True</ParameterValue>
    </MSDeployParameterValue>
    <MSDeployParameterValue Include="DefaultConnection-Web.config Connection String">
      <ParameterValue>Data Source=tcp:skyhr.database.windows.net;Initial Catalog=skyhr;User ID=skyhrAdmin;Password=IAlone@00</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>